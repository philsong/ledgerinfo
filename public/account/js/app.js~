'use strict';


angular.module('accountFilters', []).filter('checkmark', function() {
return function(input) {
  return input ? '\u2713' : '\u2718';
};
});
angular.module('txFilters', []).filter('shortTX', function() {
return function(input) {
  return shortTX(input);
};
});
angular.module('ripplenameFilters', []).filter('shortAccount', function() {
return function(input) {
  return shortAccount(input);
};
});
